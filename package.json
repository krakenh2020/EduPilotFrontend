{
  "name": "vc4sm-frontend",
  "version": "0.2022.09",
  "description": "A frontend for an API that provides education verifiable credentials to university students.",
  "main": "src/vc4sm-frontend.js",
  "license": "LGPL-2.1-or-later",
  "private": true,
  "workspaces": [
    "vendor/toolkit/packages/*"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:local",
    "i18next": "i18next-scanner",
    "watch": "rollup -c --watch",
    "watch-full": "rollup -c --watch --environment FORCE_FULL",
    "watch-bs": "rollup -c --watch --environment BUILD:bs",
    "test": "rollup -c --environment BUILD:test && karma start --singleRun",
    "test-full": "rollup -c --environment FORCE_FULL,BUILD:test && karma start --singleRun",
    "lint": "eslint ."
  },
  "repository": "git+https://github.com/krakenh2020/EduPilotFrontend.git",
